{
  "typeAcquisition": {
    "enable": true
  },
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "sourceMap": true,
    "strict": false,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node"
  }
}
